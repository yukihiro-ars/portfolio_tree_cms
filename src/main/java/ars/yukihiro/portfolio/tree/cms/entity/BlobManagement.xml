            <basic name="upDt">
                <column name="up_dt" precision="-1"/>
            </basic>
            <basic name="upNm">
                <column name="up_nm" length="20"/>
            </basic>
            <basic name="rgDt">
                <column name="rg_dt" precision="-1"/>
            </basic>
            <basic name="rgNm">
                <column name="rg_nm" length="20"/>
            </basic>
